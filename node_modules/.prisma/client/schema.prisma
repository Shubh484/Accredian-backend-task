datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id          Int      @id @default(autoincrement())
  yourName    String
  yourEmail   String
  friendName  String
  friendEmail String
  createdAt   DateTime @default(now())
}
